  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4200 --proxy-config src/proxy-conf.js -o",
    "start:prod": "ng serve --port=4200 --prod",
    "build": "ng build --outputPath=../src/main/resources/static",
    "test": "ng test",
    "test-ci": "npm run test",
    "test-dev": "ng test --watch --browsers=Chrome --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "mock:server": "json-server --watch mocks/data.json",
    "start:proxy": "ng serve --proxy-config src/proxy.conf.json",
    "start:proxy:mock:server": "concurrently --kill-others \"npm run mock:server\" \"npm run start:proxy\""
  },
